{
  "name": "momentum",
  "version": "1.0.0",
  "description": "Momentum is a scheduler that connects people to any service at the time and location of your choice!",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./bin/www",
    "startIntelliJ": "cross-env NODE_ENV=development node %NODE_DEBUG_OPTION% ./bin/www",
    "test": "cross-env NODE_ENV=development ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- test/**/*.spec.js --require test/config/tests.bootstrap --recursive -R spec",
    "standard": "standard --fix",
    "retire": " retire --node --verbose",
    "nodemon": "cross-env NODE_ENV=development nodemon ./bin/www --ignore public/ --ignore test/",
    "nsp": "nsp check",
    "build": "npm run standard && npm run retire && npm run nsp",
    "postinstall": "bower install",
    "benchmark": "node benchmark/homepage.js",
    "seed": "cross-env NODE_ENV=development node ./src/config/mongoose_seed/configuration",
    "docs": "./node_modules/.bin/apidoc -i src/routes/ -o docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ivan-Marquez/momentum.git"
  },
  "keywords": [
    "salon",
    "barber",
    "style",
    "point",
    "stylist",
    "appointment"
  ],
  "author": "Kevin Rivera, Ivan Marquez",
  "license": "PENDING",
  "bugs": {
    "url": "https://github.com/Ivan-Marquez/momentum/issues"
  },
  "homepage": "https://github.com/Ivan-Marquez/momentum#readme",
  "standard": {
    "ignore": [
      "public/"
    ]
  },
  "apidoc": {
    "title": "Momentum API Docs",
    "version": "0.1.0"
  },
  "engines": {
    "node": "^6.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "bower": "1.8.0",
    "bunyan": "1.8.5",
    "bunyan-format": "0.2.1",
    "bunyan-middleware": "0.6.0",
    "bunyan-rotating-file-stream": "1.6.1",
    "cross-env": "3.1.3",
    "dotenv": "2.0.0",
    "express": "4.14.0",
    "express-cluster": "0.0.4",
    "handlebars-form-helpers": "0.1.3",
    "hbs": "4.0.1",
    "helmet": "3.1.0",
    "http-status-codes": "1.0.6",
    "jsonwebtoken": "7.2.1",
    "lodash": "4.17.2",
    "moment": "2.17.1",
    "mongodb": "2.2.16",
    "mongoose": "4.7.4",
    "node-sass-middleware": "0.10.0",
    "nodemailer": "2.6.0",
    "passport": "0.3.2",
    "passport-jwt": "2.1.0",
    "passport-local": "1.0.0",
    "serve-favicon": "^2.3.2",
    "slugify": "1.0.2"
  },
  "devDependencies": {
    "apidoc": "0.16.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "debug": "2.4.5",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mongoose-seed": "0.3.1",
    "node-mocks-http": "1.5.5",
    "nodemon": "1.11.0",
    "nsp": "2.6.2",
    "retire": "1.2.10",
    "siege": "0.2.0",
    "sinon": "1.17.6",
    "sinon-as-promised": "4.0.2",
    "sinon-mongoose": "1.3.0",
    "standard": "*", 
    "compression": "1.6.2"
  }
}
