{
  "name": "momentum",
  "version": "1.0.0",
  "description": "Momentum is a scheduler that connects people to any service at the time and location of your choice!",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./bin/www",
    "startIntelliJ": "cross-env NODE_ENV=development node %NODE_DEBUG_OPTION% ./bin/www",
    "test": "cross-env NODE_ENV=development ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- test/**/*.spec.js --require test/config/tests.bootstrap --recursive -R spec",
    "standard": "standard --fix",
    "retire": " retire --node --verbose",
    "nodemon": "cross-env NODE_ENV=development nodemon ./bin/www --ignore public/ --ignore test/",
    "nsp": "nsp check",
    "benchmark": "node benchmark/homepage.js",
    "seed": "cross-env NODE_ENV=development node ./src/config/mongoose_seed/configuration",
    "docs": "./node_modules/.bin/apidoc -i src/routes/ -o docs/",
    "commit": "commit-wizard",
    "temp": "cross-env NODE_ENV=development node ./design/program-flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ivan-Marquez/momentum.git"
  },
  "keywords": [
    "salon",
    "barber",
    "style",
    "point",
    "stylist",
    "appointment"
  ],
  "author": "Kevin Rivera, Ivan Marquez",
  "license": "PENDING",
  "bugs": {
    "url": "https://github.com/Ivan-Marquez/momentum/issues"
  },
  "homepage": "https://github.com/Ivan-Marquez/momentum#readme",
  "standard": {
    "globals": [
      "sinon",
      "expect",
      "describe",
      "context",
      "it",
      "beforeEach",
      "assert"
    ]
  },
  "apidoc": {
    "title": "Momentum API Docs",
    "version": "0.1.0"
  },
  "engines": {
    "node": "^6.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "3.4.7",
    "body-parser": "1.16.1",
    "bower": "1.8.0",
    "bunyan": "1.8.5",
    "bunyan-format": "0.2.1",
    "bunyan-middleware": "0.6.0",
    "bunyan-rotating-file-stream": "1.6.2",
    "cross-env": "3.1.4",
    "dotenv": "4.0.0",
    "express": "4.14.1",
    "express-cluster": "0.0.4",
    "helmet": "3.4.0",
    "http-status-codes": "1.1.6",
    "jsonwebtoken": "7.3.0",
    "lodash": "4.17.4",
    "moment": "2.17.1",
    "mongodb": "2.2.24",
    "mongoose": "4.8.4",
    "node-sass-middleware": "0.11.0",
    "nodemailer": "3.1.3",
    "passport": "0.3.2",
    "passport-jwt": "2.2.1",
    "passport-local": "1.0.0",
    "slugify": "1.1.0"
  },
  "devDependencies": {
    "apidoc": "0.17.5",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "compression": "1.6.2",
    "debug": "2.6.1",
    "faker": "^4.1.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mongoose-seed": "0.3.1",
    "node-mocks-http": "1.6.1",
    "nodemon": "1.11.0",
    "nsp": "2.6.2",
    "pre-git": "^3.14.0",
    "retire": "1.2.11",
    "siege": "0.2.0",
    "sinon": "1.17.7",
    "sinon-as-promised": "4.0.2",
    "sinon-mongoose": "1.3.0",
    "standard": "8.6.0"
  },
  "config": {
    "pre-git": {
      "enabled": true,
      "allow-untracked-files": true,
      "commit-msg": "simple",
      "pre-commit": [],
      "pre-push": [
        "npm t",
        "npm run nsp"
      ],
      "post-commit": [
        "git status"
      ],
      "post-checkout": [
        "npm i"
      ],
      "post-merge": [
        "npm i"
      ]
    }
  }
}
